#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int x,y,r;
	    cin>>x>>y>>r;
	    int extra = r/30;
	    int mini = x;
	    int total = extra+mini;
	    int plates= total/y;
	    if(total%y !=0){
	     plates += 1;   
	    }
	    cout<<plates<<endl;
	}
	return 0;
}
