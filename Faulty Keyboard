class Solution {
public:
    string finalString(string s) {
        vector<char> ans;
        for(int i=0;i<s.size();i++){
            if(s[i]=='i'){reverse(ans.begin(),ans.end());}
            else ans.push_back(s[i]);
        }
        string res;
        for(int i=0;i<ans.size();i++)res+=ans[i];
        return res;
    }
};
